<!DOCTYPE html>
<html>
<head>
  <title>Random Numbers</title>
</head>
<body>
  <h2>Random Numbers</h2>
  <div id="output"></div>

  <script>
    let count = 10; // total random numbers to generate
    let i = 0;
    let outputDiv = document.getElementById("output");

    while (i < count) {
      let randomNumber = Math.floor(Math.random() * 101); // random number between 0 and 100
      outputDiv.innerHTML += "Random Number " + (i + 1) + ": " + randomNumber + "<br>";
      i++;
    }
  </script>
</body>
</html>