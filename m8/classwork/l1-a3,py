def test(lst):
    result = {} 
    for item in lst:
        result[item[0]] = item[1:]
    return result

students = [[1, 'Jean Castro', 'V'], [3, 'Brian Howell', 'VI'], [4, 'Lynne Foster', 'VI'],
[5, 'Zachay Simon', 'VII']]

print("\nOrignal list of lists:")
print(students)
print("\nConverted  lists to a dictionary:")
print(test(students))