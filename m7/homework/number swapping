# Taking input from the user
a = input("Enter the first number: ")
b = input("Enter the second number: ")
c = input("Enter the third number: ")

print("\nBefore swapping:")
print("a =", a)
print("b =", b)
print("c =", c)

# Swapping the numbers (a -> b, b -> c, c -> a)
a, b, c = c, a, b

print("\nAfter swapping:")
print("a =", a)
print("b =", b)
print("c =", c)
